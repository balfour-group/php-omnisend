{
    "name": "balfour/php-omnisend",
    "description": "A library for interacting with the Omnisend API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Goslett",
            "email": "matthew@masterstart.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Balfour\\Omnisend\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Balfour\\Omnisend\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
